										<li><a href="@Url.Action("CrmReport", "R")">Результаты продаж по неделям (CRM)</a></li>
										<li><a href="@Url.Action("CrmLegals", "R")">ИНН (CRM)</a></li>
										<li><a href="@Url.Action("Clients", "R")">Клиенты</a></li>
									</ul>
								</li>
								<li class="dropdown dropdown-submenu">
									<a href="#">Производственные</a>
									<ul class="dropdown-menu">
										<li><a href="@Url.Action("Declarations", "R")">Отчет по ДТ</a></li>
										<li><a href="@Url.Action("MailingLog", "R")">Лог рассылки</a></li>
									</ul>
								</li>
							</ul>
						</li>
						<li>
							<form class="navbar-form navbar-left" action="~/Data/Search">
								<div class="form-group">
									<input name="Number" style="width:256px; border-color:#555; background-color: #222; color:#CCC" type="text" class="form-control" placeholder="найти по номеру">
								</div>
							</form>
						</li>
					</ul>
					@Html.Partial("_LoginPartial")
				</div>
			</div>
		</div>
		<div class="container body-content">
			@RenderBody()
			<hr />
			<footer>
				<p style="font-size:smaller; color:#eee">&copy; 2016 NOVELCO LLC</p>
			</footer>
		</div>
	</div>

	@Scripts.Render("~/bundles/jquery")
	@Scripts.Render("~/bundles/bootstrap")
	@Scripts.Render("~/bundles/client")

	<script type="text/javascript">
		var app = app || {};
		app.currentBrowser = '@Request.Browser.Browser';

		$(function () { app.utility.AjaxErrorHandler(); });

		var perfData = window.performance.timing;
		var pageLoadTime = perfData.loadEventEnd - perfData.navigationStart;

		window.addEventListener("load", function() {
			setTimeout(function() {
				var timing = window.performance.timing;	// https://webperformance.ru/2011/08/22/navigation-timing-api/
				console.log("userTime = "+(timing.loadEventEnd - timing.navigationStart));
				console.log("fetchTime = "+(timing.responseEnd - timing.fetchStart));
			}, 0);
		}, false);

	</script>

	@RenderSection("scripts", required: false)
</body>
</html>

